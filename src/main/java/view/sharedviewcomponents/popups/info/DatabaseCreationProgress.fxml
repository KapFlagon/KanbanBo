<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ProgressBar?>
<?import javafx.scene.control.ProgressIndicator?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox maxHeight="-Infinity" maxWidth="-Infinity" spacing="5.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.sharedviewcomponents.popups.info.DatabaseCreationProgressPresenter">
   <children>
      <Label text="%label.file_generation.start">
         <VBox.margin>
            <Insets />
         </VBox.margin>
      </Label>
      <Label fx:id="generatedContentLbl" text="generatedContentLbl" />
      <HBox>
         <children>
            <ProgressBar fx:id="progressBar" prefWidth="180.0" progress="0.0">
               <padding>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </padding>
            </ProgressBar>
            <ProgressIndicator fx:id="progressWheel" progress="0.0" />
         </children>
         <VBox.margin>
            <Insets />
         </VBox.margin>
      </HBox>
   </children>
   <padding>
      <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
   </padding>
</VBox>
